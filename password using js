var password = "aishu";

function passcheck(){

    if(document.getElementById('pass1').value !=password){
        alert("wrong password,please try again");
        return false;
    }

    
    if(document.getElementById('pass1').value ==password){
        alert("valid,please go ahead to webpage");
    }
}
